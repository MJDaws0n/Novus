module large_counter;

// Import the macOS Silicon standard library which provides basic utilities like print and array_len.
import ../lib/standard_lib_macos_silicon;

fn main() -> i32 {
    // Start timer
    let start_ns: i64 = get_time_ns();
    let count: i32 = 0;
    while (count < 10000000000) {
        count = count + 1;
    }
    let end_ns: i64 = get_time_ns();
    print("Elapsed time (s): " + int_to_str((end_ns - start_ns) / 1000000000));

    exit(0);
    return 0;
}

